# Konfigurasi Aplikasi
APP_NAME=Laravel
APP_ENV=local
APP_KEY=base64:your-app-key-here
APP_DEBUG=true
APP_URL=http://localhost

# Konfigurasi Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# Konfigurasi Cache
CACHE_DRIVER=file
CACHE_PREFIX=laravel

# Konfigurasi Session
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Konfigurasi Queue
QUEUE_CONNECTION=sync

# Konfigurasi Mail
MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Konfigurasi Redis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Konfigurasi Filesystem
FILESYSTEM_DISK=local

# Konfigurasi Logging
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Konfigurasi Pusher (untuk broadcast)
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https

# Konfigurasi API
API_KEY=your-api-key

# Konfigurasi Layanan Eksternal
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Konfigurasi Keamanan
JWT_SECRET=your-jwt-secret

# Konfigurasi Tambahan
STRIPE_KEY=
STRIPE_SECRET=

# Konfigurasi Pengujian
TESTING_DATABASE=testing_database

# Konfigurasi Logging Eksternal
SENTRY_LARAVEL_DSN=